{
    "name": "fen grubber",
    "description": "grab fens from some webpages",
    "icons": {
        "128": "icon.png"
    },
    "version": "1.0",
    "manifest_version": 3,
    "action": {
	"default_title": "click to grub fen"
    },
    "permissions": ["activeTab", "scripting", "clipboardWrite"],
    "background": {
	"service_worker": "background.js"
    }
}
